

<section class="content-header">
    <h1>
        Test n Tags
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Test n Tags</a></li>
    </ol>
</section>
<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Add New</h3>
        </div>
        <div class="box-body ">
            <div class="col-md-6 ams-element-centered">
                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-4 control-label">Test n Tags Name:</label>

                    <div class="col-sm-8">
                        <input type="text" class="form-control" maxlength="50" id="textntagName" placeholder="Title">
                    </div>
                </div>
            </div>
        </div>

        <div class="box-footer">
            <div class="col-md-6 ams-element-centered">
                <button onclick="AMS.AddNewTag()" class="btn btn-info pull-right AddTagsBtn">Save</button>
            </div>
        </div>
    </div>

    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Test n Tags List</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>

            </div>
        </div>
        <div id="TagsContainer" class="box-body table-responsive no-padding">
        </div>
    </div>
</section>

<script>
    $(document).ready(function () {
        AMS.GetTags();
    });
</script>
